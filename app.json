{
  "expo": {
    "name": "trace-trip",
    "slug": "trace-trip",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tracetrip",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidStatusBar": {
      "backgroundColor": "#1E40AF",
      "barStyle": "light-content",
      "translucent": false
    },
    "android": {
      "package": "com.tracetrip.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1E40AF"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
